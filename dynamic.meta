<!-- PC DYNAMIC INPUT MAPPING SCHEMES FOR MINIGAMES AND OTHER SCRIPTS     -->
<!-- Redefines the script inputs on a per-script basis to create          -->
<!-- optimised PC control schemes. -->

<rage__ControlInput__DynamicMappingList>
	<DynamicMappings>


<!-- DEFAULT -->
<!-- This is used for the MP apartments and other similar activities -->
<!-- For safety any other scripts must load their own custom schemes -->

		<Item key="Default">
			<Mappings>
				<Item>
					<Input>INPUT_SCRIPT_LEFT_AXIS_X</Input> <!-- TV channel -->
					<Mappings>
						<Item>INPUT_MOVE_LR</Item>
					</Mappings>
				</Item>
				<Item>
					<Input>INPUT_SCRIPT_LEFT_AXIS_Y</Input> <!-- TV volume -->
					<Mappings>
						<Item>INPUT_MOVE_UD</Item>
					</Mappings>
				</Item>
				<Item>
					<Input>INPUT_SCRIPT_RIGHT_AXIS_X</Input>  <!-- Look -->
					<Mappings>
						<Item>INPUT_LOOK_LR</Item>
					</Mappings>
				</Item>
				<Item>
					<Input>INPUT_SCRIPT_RIGHT_AXIS_Y</Input> <!--Look -->
					<Mappings>
						<Item>INPUT_LOOK_UD</Item>
					</Mappings>
				</Item>
				<Item>
					<Input>INPUT_SCRIPT_RUP</Input> <!-- Get out of bed/shower/turn on/off tv & radio /stop dance/ -->
					<Mappings>
						<Item>INPUT_ATTACK</Item>
					</Mappings>
				</Item>
				<Item>
					<Input>INPUT_SCRIPT_RDOWN</Input> <!-- For possible future use -->
					<Mappings>
						<Item>INPUT_ATTACK</Item>
					</Mappings>
				</Item>
				<Item>
					<Input>INPUT_SCRIPT_RLEFT</Input> <!-- For possible future use -->
					<Mappings>
						<Item>INPUT_JUMP</Item>
					</Mappings>
				</Item>
				<Item>
					<Input>INPUT_SCRIPT_RRIGHT</Input> <!-- TV Stand up/stop watching -->
					<Mappings>
						<Item>INPUT_AIM</Item>
					</Mappings>
				</Item>
				<Item>
					<Input>INPUT_SCRIPT_LB</Input>
					<Mappings>
						<Item>UNDEFINED_INPUT</Item>
					</Mappings>
				</Item>
				<Item>
					<Input>INPUT_SCRIPT_RB</Input>
					<Mappings>
						<Item>UNDEFINED_INPUT</Item>
					</Mappings>
				</Item>
				<Item>
					<Input>INPUT_SCRIPT_LT</Input>
					<Mappings>
						<Item>UNDEFINED_INPUT</Item>
					</Mappings>
				</Item>
				<Item>
					<Input>INPUT_SCRIPT_RT</Input>
					<Mappings>
						<Item>UNDEFINED_INPUT</Item>
					</Mappings>
				</Item>
				<Item>
					<Input>INPUT_SCRIPT_LS</Input>
					<Mappings>
						<Item>UNDEFINED_INPUT</Item>
					</Mappings>
				</Item>
				<Item>
					<Input>INPUT_SCRIPT_RS</Input>
					<Mappings>
						<Item>UNDEFINED_INPUT</Item>
					</Mappings>
				</Item>
				<Item>
					<Input>INPUT_SCRIPT_PAD_UP</Input>
					<Mappings>
						<Item>INPUT_MOVE_UP_ONLY</Item>
					</Mappings>
				</Item>
				<Item>
					<Input>INPUT_SCRIPT_PAD_DOWN</Input>
					<Mappings>
						<Item>INPUT_MOVE_DOWN_ONLY</Item>
					</Mappings>
				</Item>
				<Item>
					<Input>INPUT_SCRIPT_PAD_LEFT</Input>
					<Mappings>
						<Item>INPUT_MOVE_LEFT_ONLY</Item> <!-- Radio prev -->
					</Mappings>
				</Item>
				<Item>
