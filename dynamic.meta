					<Mappings>
						<Item>INPUT_MOVE_RIGHT_ONLY</Item> <!-- Radio next -->
					</Mappings>
				</Item>
				<Item>
					<Input>INPUT_SCRIPT_SELECT</Input> <!-- Change camera -->
					<Mappings>
						<Item>INPUT_NEXT_CAMERA</Item>
					</Mappings>
				</Item>
			</Mappings>
		</Item>

